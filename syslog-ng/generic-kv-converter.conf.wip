filter f_meraki {
    message("\d{10}\.\d{9} (MX\d+) (\S+) (.*)")
};
rewrite rw_convert_kv {
    subst("([a-zA-Z0-9-_]+): ([a-zA-Z0-9-_\/\(\)\.: ]{3,})(,|$)",
    "$1=\"$2\"", value(MESSAGE), flags("global")
    condition( filter(f_cisco_firepower))
    );
};
([a-zA-Z0-9-_]+)=([a-zA-Z0-9-_\/\(\)\.: ']{3,})\s+
