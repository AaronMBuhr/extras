{
  "rewrite_rules": [
    {
      "match": [
        {
          "field": "program",
          "value": "Unknown"
        },
        {
          "field": "message",
          "op": "=~",
          "value": "vmware|vpxd|hostd|vsanperfsvc|Vserver|Vpxa|hostd-probe|vmafdd|ajp-nio|HandshakeCompleted"
        }
      ],
      "update": {
        "program": "VMWare"
      }
    }
  ]
}
